{
  "name": "team-ai-tutor",
  "version": "0.1.0",
  "description": "Team-Aware AI Tutor + PR Reviewer - A multi-agent system that learns coding standards and reviews PRs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ai-tutor": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:cli": "tsx src/cli/index.ts",
    "dev:web": "tsx src/web/index.ts",
    "start": "node dist/index.js",
    "start:cli": "node dist/cli/index.js",
    "start:web": "node dist/web/index.js",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "ai",
    "code-review",
    "langgraph",
    "multi-agent",
    "pr-reviewer",
    "tutor",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.1.0",
    "@langchain/langgraph": "^0.2.0",
    "@octokit/rest": "^21.0.0",
    "@supabase/supabase-js": "^2.91.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "simple-git": "^3.27.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
